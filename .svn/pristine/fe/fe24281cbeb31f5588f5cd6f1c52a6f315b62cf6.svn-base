<script type="text/javascript">
$(function(){
        var index = 0 ;
		var Swidth = 880 ;
        var timer = null ;
        var listWidth = $(".brand-list li").length*Swidth+"px";
        $(".brand-list").css('width',listWidth);
		function NextPage()
		{	
			if(index > $(".brand-list li").length-1)
			{
				index = 0 ;
			}
			$(".brand-list").stop(true, false).animate({left: -index*Swidth+"px"},800)		
		}
		function PrevPage()
		{	
			if(index<0)
			{
				index = $(".brand-list li").length-1 ;
			}
			$(".brand-list").stop(true, false).animate({left: -index*Swidth+"px"},800)		
		}
		//下一页
		$(".brand-arrow2").click(function(){
			 index++ ;
			 NextPage();
		});
		//上一页
		$(".brand-arrow1").click(function(){
			 index-- ;
			 PrevPage();
		});
		//自动滚动
		timer = setInterval(function(){
				index++ ;
				NextPage();
			},5000);
		$(".brand-arrow2 , .brand-list , .brand-arrow1").mouseover(function(){
			clearInterval(timer);
		});
		$(".brand-arrow2 , .brand-list , .brand-arrow1").mouseleave(function(){
			timer = setInterval(function(){
				index++ ;
				NextPage();
			},5000);	
		});
})
</script>
